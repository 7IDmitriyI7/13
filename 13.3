using System;

namespace _13._3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите N    ");
            int N = Convert.ToInt32(Console.ReadLine());
            int rez=0;
            for(int i=1;i<=(2*N-1); i += 2)
            {
                rez += i;
                Console.WriteLine("\n" +rez);
            }
        }
    }
}
